{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "description": "Unemployment rate over time.",
  "data": {"url": "line_unemploy.csv"},
  "mark": {
    "type": "line",
    "point": "true"
  },
  "encoding": {
    "x": {"timeUnit": "year", "field": "Date"},
    "y": {"aggregate": "mean", "field": "People", "type": "quantitative"}
  }
}
