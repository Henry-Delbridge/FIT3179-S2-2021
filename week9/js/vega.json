{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": "COVID-19 Active Cases (10 Oct 2020)",
  "width": 800,
  "height": 600,
  "data": {
    "url": "https://raw.githubusercontent.com/Henry-Delbridge/FIT3179-S2-2021/main/week9/js/wa_lga_topo.json",
    "format": {
          "type": "topojson",
          "feature": "wa_lga"
        }
  },
  "transform": [{
      "lookup": "properties.NAME",
      "from": {
        "data": {
          "url": "https://raw.githubusercontent.com/Henry-Delbridge/FIT3179-S2-2021/main/week9/js/income_lga.csv"
        },
        "key": "LGA",
        "fields": ["Income"]
      }
    }
  ],
  "projection": {
    "type": "equirectangular"
  },
  "mark": {
    "type": "geoshape"
    },
  "encoding": {
    "color": {
      "field": "Income",
      "type": "quantitative",
      "scale": {"type": "log"}
    },
    "tooltip": [
      {"field": "properties.NAME", "type": "nominal", "title": "LGA"},
      {"field": "Income", "type": "quantitative"}
    ]
  }

}